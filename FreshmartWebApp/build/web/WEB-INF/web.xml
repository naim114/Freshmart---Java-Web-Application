<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>LoginWebApp</display-name>
    <servlet>
        <servlet-name>LoginApp</servlet-name>
        <servlet-class>my.com.freshmart.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ItemAddServlet</servlet-name>
        <servlet-class>my.com.freshmart.servlets.ItemAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ItemEditServlet</servlet-name>
        <servlet-class>my.com.freshmart.servlets.ItemEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ItemDeleteServlet</servlet-name>
        <servlet-class>my.com.freshmart.servlets.ItemDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartAddServlet</servlet-name>
        <servlet-class>my.com.freshmart.servlets.CartAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartDeleteServlet</servlet-name>
        <servlet-class>my.com.freshmart.servlets.CartDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PurchaseAddServlet</servlet-name>
        <servlet-class>my.com.freshmart.servlets.PurchaseAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PurchaseRateServlet</servlet-name>
        <servlet-class>my.com.freshmart.servlets.PurchaseRateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginApp</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>my.com.freshmart.servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/addUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ItemAddServlet</servlet-name>
        <url-pattern>/addItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ItemEditServlet</servlet-name>
        <url-pattern>/editItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ItemDeleteServlet</servlet-name>
        <url-pattern>/deleteItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartAddServlet</servlet-name>
        <url-pattern>/addCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartDeleteServlet</servlet-name>
        <url-pattern>/deleteCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PurchaseAddServlet</servlet-name>
        <url-pattern>/addPurchase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PurchaseRateServlet</servlet-name>
        <url-pattern>/ratePurchase</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
